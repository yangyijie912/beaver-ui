{
  "name": "beaver-ui",
  "version": "1.1.8",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:css": "vite build --config scripts/vite.build-css.config.ts",
    "build": "pnpm run gen:tokens && pnpm run build:css && tsup && node scripts/copy-css-to-dist.mjs",
    "build:analyze": "node scripts/build-analyze.mjs",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "gen:tokens": "node scripts/generate-tokens-css.mjs",
    "check:all": "pnpm run lint && pnpm run typecheck && node scripts/check-tokens-sync.mjs && node scripts/check-naming.mjs && node scripts/find-undefined-css-vars.mjs"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "style": "dist/index.css",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./dist/style.css": "./dist/index.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "prepublishOnly": "pnpm run build",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "peerDependencies": {
    "react": ">=18.0.0 <20",
    "react-dom": ">=18.0.0 <20"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.3",
    "@storybook/addon-a11y": "10.0.8",
    "@storybook/addon-docs": "10.0.8",
    "@storybook/addon-onboarding": "10.0.8",
    "@storybook/addon-vitest": "10.0.8",
    "@storybook/react-vite": "10.0.8",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/browser-playwright": "4.0.14",
    "@vitest/coverage-v8": "4.0.14",
    "autoprefixer": "^10.4.22",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-storybook": "10.0.8",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "10.0.8",
    "ts-node": "^10.9.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14",
    "esbuild": "^0.19.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "date-fns": "^4.1.0"
  }
}
